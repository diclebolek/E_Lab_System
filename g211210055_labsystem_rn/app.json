{
  "expo": {
    "name": "E-Laboratuvar Sistemi",
    "slug": "g211210055-labsystem-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0058A3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.labsystem.elab"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0058A3"
      },
      "package": "com.labsystem.elab"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Fotoğraflara erişim için izin gerekiyor.",
          "cameraPermission": "Kamera erişimi için izin gerekiyor."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Kamera erişimi için izin gerekiyor."
        }
      ]
    ]
  }
}
